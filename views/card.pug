doctype html
html
	head
		title= name
		link(rel="stylesheet" href="/static/card.css")/
		include include/head.pug
	body
		a(href='/')
			img(src='/static/return.png' width='48px' height='48px' alt='Return to Indexicon' left='25px')
		.center
			h1= name
			.bit
				img(src=image width='64px' height='64px' alt=printing right-10px)
			.effect
				p= effect
				.power
					.p2= power
				.deckslot
					.p2= deckslot
				.rating
					.p2= rating
			.unlock= unlock
